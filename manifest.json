{
  "name": "Sanu Voice Automation",
  "version": "1.0.0",
  "description": "Automate basic function of web with your voice command",
  "manifest_version": 3,
  "author": "Pranjal Tiwari",
  "icons": { "16": "/icons.png", "48": "/icons.png", "128": "/icons.png" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Setting Panel"
  }
}
